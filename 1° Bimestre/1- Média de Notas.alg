algoritmo "Média de Notas"

var
nota1, nota2, nota3, mediaf : real

inicio
   escreva ("Informe a nota 1: ")
   leia (nota1)
   escreva ("Informe a nota 2: ")
   leia (nota2)
   escreva ("Informe a nota 3: ")
   leia (nota3)
   
   mediaf <- (nota1 + nota2 + nota3) /3
   
   escreva ("A média final é: ", mediaf)

fimalgoritmo
